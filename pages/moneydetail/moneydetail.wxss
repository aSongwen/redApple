.header{
  display: flex;
  height: auto;
  flex-direction: column;
  align-items: center;
  width: 100%;
  background-color: rgba(240,240,240,.7);
}
.headere-text1{
  font-size: 64rpx;
  color: #333333;
}
.headere-text2{
  font-size: 30rpx;
  color: #5d5d5d;
}
.one{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: rgba(240,240,240,.7);
  padding-bottom: 20rpx;
}
.one-row{
  width: 90%;
  height: auto;
  display: flex;
  align-items: center;
}
.one-row-row1{
  width: 30%;
  display: flex;
  font-size: 32rpx;
  color: #999999;
}
.one-row-row2{
  width: 70%;
  display: flex;
  flex-direction: row-reverse;
  font-size: 32rpx;
  color: #333333;
}