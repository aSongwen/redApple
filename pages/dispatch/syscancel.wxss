}

.show-view {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f0f0f0;
}

.header {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 80rpx;
  width: 100%;
  background-color: #ffcccc;
}

.header-view {
  width: 50rpx;
