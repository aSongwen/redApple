.modal-dialog2 {
  width: 100%;
  background: #f9f9f9;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
}
.modal-title {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #333333;
}
.modal-content{
  background-color: #ffffff;
}
.modal-content2{
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  align-items: center;
  width: 100%;
}
.modal-center{
  display: flex;
  align-items: center;
  width: 80%;
  border-bottom: 1rpx solid #cdcdcd;
  height: 100rpx;
}
.modal-center:last-child{
  display: flex;
  align-items: center;
  width: 80%;
  height: 100rpx;
  border-bottom:none;
}
.modal-row1{
  display: flex;
  width: 50%;
  height: 100%;
  flex-direction: column;
  justify-content: center;
}
.modal-row2{
  display: flex;
  width: 50%;
  height: 100%;
  flex-direction: row-reverse;
  align-items: center;
}
.modal-row1-text1{
  font-size: 30rpx;
  color: #333333;
}
.modal-row1-text2{
  font-size: 24rpx;
  color: #999999;
}
.modal-row1-text3{
  font-size: 30rpx;
}
.bottom{
  display: flex;
  width: 100%;
  height: 50rpx;
  justify-content: center;
  align-items: center;
}
.shareBtn{
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0;
  padding: 0;
}
.shareBtn::after{
  border-width: 0px;
}