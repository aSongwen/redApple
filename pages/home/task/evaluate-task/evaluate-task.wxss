.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 500rpx;
  position: fixed;
  top: 40%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 125rpx;
  border-radius: 36rpx;
  padding: 40rpx 60rpx;
  box-sizing: border-box;
}

.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-content {
 color:#666;
  font-size: 28rpx;
  margin-bottom: 50rpx;
  line-height: 55rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  font-size: 34rpx;
  line-height: 86rpx;
}
.modal-footer button::after{
  border: none;
}
.dialogImg{
  display: flex;
  justify-content: center;
}
.dialogImg image{
  width:280rpx;
  height:200rpx;
  margin-bottom: 60rpx;
}
.btn-cancel {
  width: 50%;
  color: #e35441;
  text-align: center;
  font-size: 28rpx;
}

.btn-confirm {
  width: 50%;
  color: #fff;
  height:66rpx;
  line-height: 66rpx;
  text-align: center;
  font-size: 28rpx;
  background: #ff7e77;
  border-bottom: 6rpx solid #e3544c;
  border-radius: 30rpx;
}
.dialogClose{
  display: flex;
  justify-content: center;
  position: absolute;
  bottom: -100rpx;
  width:380rpx;
  z-index: 9001;
}
.dialogClose image{
  width:50rpx;
  height:50rpx;
  z-index: 9001;
}