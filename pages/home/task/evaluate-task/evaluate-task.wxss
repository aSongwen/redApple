.show-view {
  /*background-color: rgba(240,240,240,.6);*/
  display: flex;
  flex-direction: column;
}
.headers {
  position: fixed;
  display: flex;
  top: 0;
  left: 0;
  height: 100rpx;
  width: 100%;
  background-color: #ffe5d3;
  border-bottom: 0.5rpx solid #ffc096;
  z-index: 6666;
}

.headerows-row1 {
  display: flex;
  height: 100%;
  width: 15%;
  justify-content: center;
  /* margin-top: 30rpx; */
  align-items: center;
}

.headerows-row2 {
  display: flex;
  flex-direction: column; 
  justify-content: center;
  height: 100%;
  width: 55%;
  /* margin-top: 30rpx; */
}

.headerows-row2-column1 {
  color: #5d5d5d;
  height: auto;
  font-size: 30rpx;
}

.headerows-row2-column2 {
  color: #999;
  font-size: 32rpx;
  height: 50%;
}

.headerows-row3 {
  display: flex;
  height: 100%;
  align-items: center;
  width: 30%;
  justify-content: center;
}
.bottom-view {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 120rpx;
  background-color: #ffffff;
  line-height: 120rpx;
  z-index: 66;
  padding-top: 30rpx;
  border-top: 0.5rpx #cdcdcd solid;
}

.bottom-view-btn {
  width: 90%;
  height: 80%;
  color: #ffffff;
  background-color: #f54747;
}
.second {
  display: flex;
  width: 100%;
  height: 100rpx;
  color: #5d5d5d;
  align-items: center;
}

.icon {
  height: 42rpx;
  width: 8rpx;
  border-left: 8rpx solid #f54747;
}
.three {
  display: flex;
