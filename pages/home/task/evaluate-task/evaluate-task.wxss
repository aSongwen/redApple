}
.four-column2{
  width: 100%;
  display: flex;
  align-items: center;
}
.four-column2-textarea{
  width: 90%;
  height: 100%;
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #333;
}
.four-column3{
  /* height: auto; */
  width: 100%;
  display: flex;
  justify-content: center;
  /*background-color:rgba(255,255,255,.7);*/
}
.four-column3-head{
  height: auto;
  width: 90%;
  display: flex;
  flex-wrap: wrap;
}
.four-column3-array{
  height: 180rpx;
  width: 90%;
}
.four-column3-array-image{
  height: 180rpx;
