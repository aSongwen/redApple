}

.three-two {
  display: flex;
  flex-direction: column;
  width: 60%;
  height: 100%;
}

.three-two-row1 {
  display: flex;
  padding-top: 25rpx;
  height: 80rpx;
  color: #333333;
  font-size: 36rpx;
}
.three-two-row1-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.three-two-row2 {
  display: flex;
  align-items: center;
  height: 60rpx;
  color: #999999;
  font-size: 30rpx;
}
.three-header {
  display: flex;
  align-items: center;
  height: 80rpx;
  width: 100%;
  background-color:rgba(255,255,255,.7);
  border-bottom: 0.5rpx #cdcdcd solid;
  color: #999999;
  font-size: 30rpx;
}
.four{
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color:rgba(255,255,255,.7);
}

.four-column1{
  height: 60rpx;
