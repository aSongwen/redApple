/*
  遮罩层样式
*/

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

/*
  弹窗样式
*/

.modal-dialog {
  width: 80%;
  overflow: hidden;
  position: fixed;
  top: 30%;
  left: 10%;
  z-index: 9999;
  background: #f9f9f9;
  border-radius: 10rpx;
}

.modal-title {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
}

.modal-input {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 200rpx;
  /* line-height: 200rpx; */
  text-align: center;
}

.modal-input-text {
  color: #5d5d5d;
  font-size: 30.5rpx;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 100rpx;
  border-top: 0.5rpx solid #dedede;
  font-size: 36rpx;
  line-height: 100rpx;
}

